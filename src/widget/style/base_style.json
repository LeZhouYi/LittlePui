{
    "$colorBase": "white",
    "$colorTheme": "#ADD8E6",
    "$colorText": "#1B5765",
    "$colorThemeLight": "#C0DBF1",

    "$fontSmall": ["微软雅黑", 15],
    "$fontBase": ["微软雅黑", 16],
    "$fontLarge": ["微软雅黑", 18],

    "$cnfBase":{
        "background": "$colorBase"
    },
    "$cnfBtnBase":{
        "background": "$colorTheme",
        "font": "$fontSmall",
        "fg": "$colorBase"
    },
    "$cnfLabelBase":{
        "background": "$colorTheme",
        "font": "$fontBase",
        "fg": "$colorBase"
    },

    "$packBothNW": {
        "fill": "both",
        "side": "left",
        "anchor": "nw",
        "expand": 1
    },
    "$packYLN":{
        "fill": "y",
        "side": "left",
        "anchor": "n"
    },
    "$packXTC":{
        "fill": "x",
        "side": "top",
        "anchor": "center"
    },
    "$packXTNW":{
        "fill": "x",
        "side": "top",
        "anchor": "nw"
    },
    "$packXTW":{
        "fill": "x",
        "side": "top",
        "anchor": "w"
    },
    "$packYLNE":{
        "fill": "y",
        "side": "right",
        "anchor": "e"
    },
    "$packEMLNW":{
        "fill": "none",
        "side": "left",
        "anchor": "nw"
    },
    "$packEMLW":{
        "fill": "none",
        "side": "left",
        "anchor": "w"
    },
    "$packEMRC":{
        "fill": "none",
        "side": "right",
        "anchor": "center"
    },
    "$packEMTC":{
        "fill": "none",
        "side": "top",
        "anchor": "center"
    },
    "$packEMRE":{
        "fill": "none",
        "side": "right",
        "anchor": "e"
    },
    "$packEMTW":{
        "fill": "none",
        "side": "top",
        "anchor": "w"
    },

    "cfmDlg":{
        "cnf":{
            "background": "$colorTheme",
            "height": 252,
            "width": 164
        },
        "packCnf": null,
        "type": "Dialog"
    },
    "cfmFrame":{
        "cnf":{
            "background": "$colorBase",
            "borderwidth": 40
        },
        "packCnf":{
            "fill": "none",
            "side": null,
            "anchor": "center"
        },
        "type": "Frame"
    },
    "cfmLabel":{
        "cnf":{
            "background": "$colorBase",
            "font": "$fontBase",
            "fg": "$colorText",
            "anchor": "center",
            "height": 2
        },
        "packCnf":"$packEMTC",
        "type": "Label"
    },
    "cfmYesBtn":{
        "cnf":{
            "background": "$colorBase",
            "font": "$fontBase",
            "fg": "$colorText",
            "anchor": "w",
            "width": 8,
            "height": 2
        },
        "packCnf":"$packEMLW",
        "type": "Label"
    },
    "cfmNoBtn":{
        "cnf":{
            "background": "$colorBase",
            "font": "$fontBase",
            "fg": "$colorText",
            "anchor": "e",
            "width": 8,
            "height": 2
        },
        "packCnf":"$packEMRE",
        "type":"Label"
    },
    "cfmContent":{
        "cnf":{
            "background": "$colorBase",
            "borderwidth": 1
        },
        "packCnf":"$packXTC",
        "type": "Frame"
    },
    "cfmInfo":{
        "cnf":{
            "background": "$colorBase",
            "font": "$fontBase",
            "fg": "$colorText",
            "anchor": "w",
            "height": 2,
            "width": 50
        },
        "packCnf":"$packEMTW",
        "type": "Label"
    },
    "cfmLineFrame":{
        "cnf":{
            "background": "$colorBase"
        },
        "packCnf":"$packXTC",
        "type": "Frame"
    },
    "cfmEntryLabel":{
        "cnf":{
            "background": "$colorBase",
            "font": "$fontBase",
            "fg": "$colorText",
            "anchor": "e",
            "width": 8
        },
        "packCnf":"$packEMLW",
        "type": "Label"
    },
    "cfmEntry":{
        "cnf":{
            "background": "$colorBase",
            "font": "$fontBase",
            "width": 8,
            "fg": "$colorText"
        },
        "packCnf":{
            "$ref":"$packXTW",
            "padx": 1,
            "pady": 1
        },
        "type": "Entry"
    }
}