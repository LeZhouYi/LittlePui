{
    "imports":[
        "src/widget/style/base_style.json"
    ],
    "sideBarBtn":{
        "cnf":{
            "width": 15,
            "height": 2,
            "$ref": "$cnfBtnBase"
        },
        "packCnf":"$packXTC",
        "type": "Label"
    },
    "pwdDispalyFrame":{
        "cnf":"$cnfBase",
        "packCnf":"$packBothNW",
        "type": "Frame"
    },
    "pwdScrollCanvas":{
        "cnf":{
            "background": "$colorBase",
            "highlightthickness":0,
            "borderwidth":2
        },
        "packCnf": "$packEMLNW",
        "type": "Canvas"
    },
    "pwdContentFrame":{
        "cnf":"$cnfBase",
        "packCnf": null,
        "type": "Frame"
    },
    "pwdScrollBar":{
        "cnf":{
            "background":"$colorBase",
            "orient": "vertical",
            "width":1
        },
        "packCnf":"$packYLNE",
        "type":"ScrollBar"
    },
    "pwdSingleFrame":{
        "cnf":"$cnfBase",
        "packCnf":{
            "$ref": "$packXTNW",
            "ipady":8
        },
        "type": "Frame"
    },
    "pwdLine":{
        "cnf":{
            "background": "$colorTheme"
        },
        "packCnf":"$packXTNW",
        "type": "Frame"
    },
    "pwdGroupLabel":{
        "cnf":{
            "$ref":"$cnfLabelBase",
            "anchor": "w"
        },
        "packCnf":{
            "$ref":"$packXTNW",
            "padx": 3,
            "pady": 8
        },
        "type": "Label"
    },
    "pwdBtn":{
        "cnf":{
            "background": "$colorTheme",
            "anchor": "center"
        },
        "packCnf":{
            "$ref":"$packEMRC",
            "padx": 5,
            "pady": 5
        },
        "type": "Label"
    },
    "pwdEnvFrame":{
        "cnf":"$cnfBase",
        "packCnf":{
            "$ref":"$packXTNW",
            "padx": 1,
            "pady": 5
        },
        "type": "Frame"
    },
    "pwdEnvLineFrame":{
        "cnf":"$cnfBase",
        "packCnf":"$packXTNW",
        "type": "Frame"
    },
    "pwdEmptyLabel":{
        "cnf":{
            "background": "$colorBase",
            "font": "$fontBase",
            "anchor": "w",
            "width": 3
        },
        "packCnf":"$packEMLNW",
        "type": "Label"
    },
    "pwdEnvLabel":{
        "cnf":{
            "$ref":"$cnfLabelBase",
            "font": "$fontLarge",
            "anchor": "w"
        },
        "packCnf":"$packXTNW",
        "type": "Label"
    },
    "pwdEnvBtn":{
        "cnf":{
            "$ref":"$cnfLabelBase",
            "font": "$fontLarge",
            "anchor": "center",
            "height":20,
            "width": 25
        },
        "packCnf":"$packEMRC",
        "type": "Label"
    },
    "pwdDataFrame":{
        "cnf":{
            "background": "$colorBase",
            "borderwidth": 2,
            "relief":"solid"
        },
        "packCnf":{
            "$ref":"$packXTNW",
            "padx": 1,
            "pady": 3
        },
        "type": "Frame"
    },
    "pwdItemFrame":{
        "cnf":"$cnfBase",
        "packCnf":{
            "$ref":"$packXTNW",
            "padx": 1,
            "pady": 1
        },
        "type": "Frame"
    },
    "pwdItemLabel":{
        "cnf":{
            "background": "$colorBase",
            "font": "$fontBase",
            "anchor": "e",
            "width": 8,
            "fg":"$colorText"
        },
        "packCnf":{
            "$ref":"$packEMLW",
            "padx": 1,
            "pady": 1
        },
        "type":"Label"
    },
    "pwdItemValue":{
        "cnf":{
            "background": "$colorBase",
            "font": "$fontBase",
            "width": 8,
            "fg": "$colorText"
        },
        "packCnf":{
            "$ref":"$packXTW",
            "padx": 1,
            "pady": 1
        },
        "type":"Entry"
    },
    "pwdItemBtn":{
        "cnf":{
            "$ref":"$cnfBtnBase",
            "font":"$fontBase",
            "anchor": "center"
        },
        "packCnf":{
            "$ref":"$packEMRC",
            "padx": 5,
            "pady": 1
        },
        "type": "Label"
    },
    "pwdValueLabel":{
        "cnf":{
            "background": "$colorThemeLight",
            "font": "$fontBase",
            "width": 8,
            "fg": "$colorText"
        },
        "packCnf":{
            "$ref":"$packEMLW",
            "padx": 3,
            "pady": 1
        },
        "type":"Label"
    }
}